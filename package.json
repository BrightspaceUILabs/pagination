{
  "name": "@brightspace-ui-labs/pagination",
  "version": "0.0.5",
  "description": "A component to indicate the existence of multiple pages of content, and to provide a mechanism to navigate them.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BrightspaceUILabs/pagination.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "pagination.js",
    "localization.js",
    ".gitignore",
    "/lang"
  ],
  "scripts": {
    "lint": "npm run lint:eslint && npm run lint:lit",
    "lint:eslint": "eslint . --ext .js,.html",
    "lint:lit": "lit-analyzer pagination.js demo test",
    "start": "es-dev-server --app-index demo/index.html --node-resolve --dedupe --open --watch",
    "test": "npm run lint && npm run test:local",
    "test:local": "polymer test --skip-plugin sauce",
    "test:sauce": "polymer test --skip-plugin local",
    "test:travis": "polymer test --config-file wct.conf-travis.json"
  },
  "author": "D2L Corporation",
  "license": "Apache-2.0",
  "devDependencies": {
    "@brightspace-ui/visual-diff": "^1",
    "@open-wc/testing": "^2",
    "@open-wc/testing-karma": "^3",
    "babel-eslint": "^10",
    "deepmerge": "^3",
    "es-dev-server": "^1",
    "eslint": "^6",
    "eslint-config-brightspace": "^0.6.7",
    "eslint-plugin-html": "^6",
    "eslint-plugin-lit": "^1",
    "eslint-plugin-sort-class-members": "^1",
    "frau-ci": "^1",
    "karma-sauce-launcher": "^2",
    "lit-analyzer": "^1",
    "puppeteer": "^1"
  },
  "dependencies": {
    "@brightspace-ui/core": "^1.43.4",
    "@polymer/polymer": "^3.4.1",
    "@polymer/test-fixture": "^4.0.2",
    "@webcomponents/webcomponentsjs": "^2",
    "lit-element": "^2",
    "polymer-cli": "^1.9.11",
    "wct-mocha": "^1.1.0"
  },
  "homepage": "https://github.com/BrightspaceUILabs/paginatione#readme",
  "main": "index.js",
  "directories": {
    "test": "test"
  }
}
